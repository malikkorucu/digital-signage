{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-undef": 1,
    "no-extra-semi": 1,
    "no-console": [1, { "allow": ["warn", "error"] }],
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "quotes": [1, "single", "avoid-escape"],
    "semi": ["error", "never"]
  }
}
